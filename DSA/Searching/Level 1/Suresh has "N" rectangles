#include<iostream>
using namespace std;
#define ll long long int
int main()
{
    ll n,ans=0; 
    cin>>n;
    
    while(n--)
    {
        double width,height;
        cin>>width>>height;
        
        if(width/height>=1.6 && width/height<=1.7)
        ans++;
        
        
        else if(height/width >=1.6 && height/width<=1.7)
        ans++;
}
cout<<ans<<endl;
	return 0;
}
